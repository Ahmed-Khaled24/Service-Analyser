<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="JAVAFX_Controllers.Output_Window"
            stylesheets="/Style.css"
            >
    <right>

        <VBox spacing="5" prefWidth="500">
           <Label text="Components" fx:id="ComponentsLabel" VBox.vgrow="ALWAYS"
                  style="-fx-border-color: Silver" >
               <padding>
                    <Insets  left="5" top="5" bottom="5"/>
               </padding>

            </Label>


<!--                <TextArea  editable="false" VBox.vgrow="ALWAYS"  />-->
<!--                <TextArea  editable="false" VBox.vgrow="ALWAYS"  />-->



        <TextArea  editable="false" VBox.vgrow="ALWAYS" fx:id="TextArea"  />
        </VBox>

    </right>




    <center>
        <VBox prefWidth="500">

            <Label text="Request  Objects" fx:id="RequestLabel"
                   style="-fx-border-color: Silver" >
                <padding>
                    <Insets  left="5" top="5" bottom="5"/>
                </padding>
            </Label>

            <ListView fx:id="RequestField_Names" VBox.vgrow="ALWAYS" prefWidth="500">
            </ListView>

            <Label text="Response  Objects" fx:id="ResponseLabel"
                   style="-fx-border-color: Silver" prefWidth="500">
                <padding>
                    <Insets  left="5" bottom="5" top="5"/>
                </padding>

            </Label>
            <ListView fx:id="ResponseField_Names" VBox.vgrow="ALWAYS" >
            </ListView>

        </VBox>

    </center>




    <left>
        <VBox prefWidth="500">
        <Label text="APIs " fx:id="API_Label" style="-fx-border-color: Silver" prefWidth="500">
            <padding>
                <Insets  left="5" top="5" bottom="5"/>
            </padding>

        </Label>


        <ListView fx:id="API_NAMES"  VBox.vgrow="ALWAYS" prefWidth="500"  >

        </ListView>
        </VBox>
    </left>



</BorderPane>
